(define-abbrev-table 'global-abbrev-table
  '(
    ("ss" "§ ───────────────────────────────────────────────────────────────────────────────────" nil :count 2)
   ))

(define-abbrev-table 'julia-mode-abbrev-table
  '(
    ("\\ss" "ß" nil :count 1)
    ("\\times" "×" nil :count 1)
    ("\\yen" "¥" nil :count 5)
    ("in" "∈" nil :count 6)
   ))

